CREATE TABLE memberList (
userID int NOT NULL AUTO_INCREMENT,
firstName varchar(25) DEFAULT NULL,
email varchar(50),
username varchar(20),
password varchar(255),
char1 varchar(12) DEFAULT NULL,
char2 varchar(12) DEFAULT NULL,
char3 varchar(12) DEFAULT NULL,
char4 varchar(12) DEFAULT NULL,
PRIMARY KEY (userID));

CREATE TABLE rank(
userName varchar(12),
overall int(15),
attack int,
defence int,
strength int,
hitpoints int,
ranged int,
prayer int,
magic int,
cooking int,
woodcutting int,
fletching int,
fishing int,
firemaking int,
crafting int,
smithing int,
mining int,
herblore int,
agility int,
thieving int,
slayer int,
farming int,
runecraft int,
hunter int,
construction int,
PRIMARY KEY (userID));

CREATE TABLE level(
userName varchar(12),
overall int(4),
attack int,
defence int,
strength int,
hitpoints int,
ranged int,
prayer int,
magic int,
cooking int,
woodcutting int,
fletching int,
fishing int,
firemaking int,
crafting int,
smithing int,
mining int,
herblore int,
agility int,
thieving int,
slayer int,
farming int,
runecraft int,
hunter int,
construction int,
PRIMARY KEY (userID));

CREATE TABLE experience(
userName varchar(12),
overall int(20),
attack int(9),
defence int(9),
strength int(9),
hitpoints int(9),
ranged int(9),
prayer int(9),
magic int(9),
cooking int(9),
woodcutting int(9),
fletching int(9),
fishing int(9),
firemaking int(9),
crafting int(9),
smithing int(9),
mining int(9),
herblore int(9),
agility int(9),
thieving int(9),
slayer int(9),
farming int(9),
runecraft int(9),
hunter int(9),
construction int(9),
PRIMARY KEY (userID));

CREATE TABLE bounty(
userName varchar(12),
huntRank int,
huntKills int,
rogueRank int,
rogueKills int,
PRIMARY KEY (userID));

CREATE TABLE clues(
userName varchar(12),
beginnerRank int(12),
beginnerDone int(6),
easyRank int(12),
easyDone int(6),
mediumRank int(12),
mediumDone int(6),
hardRank int(12),
hardDone int(6),
eliteRank int(12),
eliteDone int(6),
masterRank int(12),
masterDone int(6),
overallRank int(12),
overallDone int(6),
PRIMARY KEY (userID));

CREATE TABLE itemStats(
itemID int(3) NOT NULL AUTO_INCREMENT,
itemName varchar(30),
itemSlot varchar(17),
stabAttack int(3),
slashAttack int(3),
crushAttack int(3),
rangeAttack varchar(20),
magicAttack int(3),
stabDefence int(3),
slashDefence int(3),
crushDefence int(3),
rangeDefence int(3),
magicDefence int(3),
meleeStrength int(3),
rangeStrength varchar(20),
magicStrength int(3),
prayer varchar(10),
attackSpeed varchar(10) DEFAULT NULL,
PRIMARY KEY (itemID,itemName));

CREATE TABLE pkBuilds(
buildName varchar(30),
buildIcon text,
health varchar(15),
attack varchar(15),
strength varchar(15),
defence varchar(15),
ranged varchar(15),
prayer varchar(15),
mage varchar(15),
notes text,
PRIMARY KEY (buildName));

CREATE TABLE exchange(
itemID int (5) NOT NULL,
name varchar(60),
members bool,
shopPrice int(50),
buyAverage int(50),
buyQuantity int(50),
sellAverage int(50),
sellQuantity int(50),
overallAverage int(50),
overallQuantity int(60),
PRIMARY KEY (itemID));

CREATE TABLE alchemy(
itemID int(5) NOT NULL,
name varchar(60),
value int(50),
highAlchemy int(50),
lowAlchemy int(50),
PRIMARY KEY (itemID));






